{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2020,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		}
	},
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"no-mixed-spaces-and-tabs": 0 // disable rule
	}
}
